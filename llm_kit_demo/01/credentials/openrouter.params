# ===== ОСНОВНЫЕ НАСТРОЙКИ LLM =====

url = https://openrouter.ai/api/v1/chat/completions         # API-эндпоинт сервиса
api_key = укажите_ваш_секретный_api_ключ                    # Ваш секретный API-ключ с OpenRouter
model = deepseek/deepseek-chat                              # Название модели (например: deepseek/deepseek-r1:free)

# ===== ДОПОЛНИТЕЛЬНЫЕ ПАРАМЕТРЫ =====

seed = 42                                                   # Установка seed для воспроизводимости генерации ответа
temperature = 0.5                                           # Температура (0.0-2.0): ниже — строже и предсказуемее, выше — больше креатива и разнообразия
max_tokens = 25                                             # Максимальное количество токенов в ответе